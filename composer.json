{
  "name": "visacheck/laravel-auth",
  "homepage": "https://visacheck.co",
  "description": "Extension to integrate user authentication via the Visacheck API to Laravel applications",
  "keywords": ["visacheck","business","vehicles","ride hailing", "laravel"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Emmanuel Okeke",
      "homepage": "https://github.com/emmanix2002"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/emmanix2002/visacheck-sdk-php.git"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/auth": "5.5.*",
    "visacheck/visacheck-sdk-php": "dev-master",
    "illuminate/notifications": "5.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload": {
    "psr-4": {
      "Visacheck\\Visacheck\\LaravelAuth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Visacheck\\Visacheck\\LaravelAuth\\VisacheckAuthServiceProvider"
      ]
    }
  }
}
